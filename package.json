{
  "name": "bun-rest-app",
  "version": "1.0.50",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "bun run --watch src/bootstrap.ts | pino-pretty",
    "lint": "biome lint --write ./src",
    "format": "biome format --write ./src",
    "migrate:run": "bun run src/adapters/db/scripts/migrate.ts",
    "migrate:create": "bun run src/adapters/db/scripts/create.ts",
    "build": "bun build --compile --minify-whitespace --minify-syntax --target bun-linux-x64 --outfile server src/bootstrap.ts"
  },
  "dependencies": {
    "@bogeychan/elysia-logger": "^0.1.10",
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/jwt": "^1.4.0",
    "@elysiajs/openapi": "^1.4.11",
    "@types/pg": "^8.15.6",
    "elysia": "latest",
    "kysely": "^0.28.8",
    "pg": "^8.16.3",
    "pino": "^10.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.4",
    "@types/bun": "^1.3.2",
    "@types/node": "^24.10.0",
    "bun-types": "latest",
    "pino-pretty": "^13.1.2",
    "typescript": "^5.9.3"
  },
  "module": "src/index.js",
  "type": "module"
}
